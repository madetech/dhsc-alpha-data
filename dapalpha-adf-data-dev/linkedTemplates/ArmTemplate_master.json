{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dapalpha-adf-data-dev"
		},
		"Analytical_Datastore_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dapalpha-sql-data-dev.database.windows.net"
		},
		"Analytical_Datastore_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Analytical_Datastore"
		},
		"Datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dapalphadatastlakedev.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dapalpha-adf-data-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Analytical_Datastore_properties_typeProperties_server": {
						"value": "[parameters('Analytical_Datastore_properties_typeProperties_server')]"
					},
					"Analytical_Datastore_properties_typeProperties_database": {
						"value": "[parameters('Analytical_Datastore_properties_typeProperties_database')]"
					},
					"Datalake_properties_typeProperties_url": {
						"value": "[parameters('Datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}