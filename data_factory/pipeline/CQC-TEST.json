{
	"name": "CQC-TEST",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['total']"
								}
							},
							{
								"source": {
									"path": "$['firstPageUri']"
								}
							},
							{
								"source": {
									"path": "$['page']"
								}
							},
							{
								"source": {
									"path": "$['previousPageUri']"
								}
							},
							{
								"source": {
									"path": "$['lastPageUri']"
								}
							},
							{
								"source": {
									"path": "$['nextPageUri']"
								}
							},
							{
								"source": {
									"path": "$['perPage']"
								}
							},
							{
								"source": {
									"path": "$['totalPages']"
								}
							},
							{
								"source": {
									"path": "$['providers'][0]['providerId']"
								}
							},
							{
								"source": {
									"path": "$['providers'][0]['providerName']"
								}
							},
							{
								"source": {
									"path": "$['uri']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "RestResource1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}