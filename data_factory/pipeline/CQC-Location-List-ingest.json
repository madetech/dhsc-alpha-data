{
	"name": "CQC-Location-List-ingest",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"QueryParameters.{page}": "RANGE:1::1"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['locationId']"
								},
								"sink": {
									"name": "locationId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['locationName']"
								},
								"sink": {
									"name": "locationName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['postalCode']"
								},
								"sink": {
									"name": "postalCode",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['locations']"
					}
				},
				"inputs": [
					{
						"referenceName": "CQC_location_list",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SQL_CQC_LOCATION_LIST",
						"type": "DatasetReference"
					}
				]
			}
		],
		"variables": {
			"PageNumber": {
				"type": "Integer"
			},
			"TotalPages": {
				"type": "Integer"
			}
		},
		"annotations": []
	}
}